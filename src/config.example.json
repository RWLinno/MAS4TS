{
  "system": {
    "device": "cuda",
    "use_multi_gpu": false,
    "gpu_ids": [0],
    "seed": 2021,
    "num_workers": 10
  },
  "data": {
    "seq_len": 96,
    "pred_len": 96,
    "label_len": 48,
    "use_norm": true,
    "smooth_window": 5,
    "use_differencing": false,
    "seasonal_period": 12
  },
  "agents": {
    "manager_agent": {
      "enabled": true,
      "use_parallel_agents": true,
      "agent_priority": {
        "data_analyzer": 1,
        "visual_anchor": 2,
        "numerologic_adapter": 3,
        "task_executor": 4
      }
    },
    "data_analyzer": {
      "enabled": true,
      "anomaly_threshold": 3.0,
      "smooth_window": 5,
      "use_differencing": false
    },
    "visual_anchor": {
      "enabled": true,
      "use_vlm": false,
      "vlm_model": "Qwen/Qwen2.5-VL-7B-Instruct",
      "anchor_strategy": "confidence_interval",
      "confidence_level": 0.95,
      "fig_size": [10, 6],
      "dpi": 100,
      "plot_style": "default"
    },
    "numerologic_adapter": {
      "enabled": true,
      "hidden_dim": 128,
      "num_layers": 2,
      "fusion_strategy": "attention"
    },
    "knowledge_retriever": {
      "enabled": true,
      "use_vector_db": false,
      "k_neighbors": 5,
      "similarity_threshold": 0.7
    },
    "task_executor": {
      "enabled": true,
      "default_model": "DLinear"
    }
  },
  "models": {
    "default_model": "DLinear",
    "model_config": {
      "d_model": 512,
      "n_heads": 8,
      "e_layers": 2,
      "d_layers": 1,
      "d_ff": 2048,
      "dropout": 0.1,
      "activation": "gelu"
    },
    "supported_models": [
      "DLinear",
      "TimesNet",
      "Autoformer",
      "Transformer",
      "Informer",
      "PatchTST",
      "iTransformer",
      "TimeMixer",
      "TSMixer",
      "FEDformer"
    ]
  },
  "tasks": {
    "forecasting": {
      "enabled": true,
      "pred_lens": [96, 192, 336, 720],
      "metrics": ["mse", "mae", "rmse", "mape", "mspe"]
    },
    "classification": {
      "enabled": true,
      "metrics": ["accuracy", "precision", "recall", "f1"]
    },
    "imputation": {
      "enabled": true,
      "mask_rate": 0.25,
      "metrics": ["mse", "mae"]
    },
    "anomaly_detection": {
      "enabled": true,
      "anomaly_ratio": 0.25,
      "metrics": ["precision", "recall", "f1", "auc"]
    }
  },
  "training": {
    "train_epochs": 10,
    "batch_size": 32,
    "learning_rate": 0.0001,
    "patience": 3,
    "use_amp": false,
    "loss": "MSE",
    "lradj": "type1",
    "itr": 1
  },
  "datasets": {
    "ETTh1": {
      "root_path": "./dataset/ETT-small/",
      "data_path": "ETTh1.csv",
      "features": "M",
      "target": "OT",
      "freq": "h",
      "enc_in": 7,
      "dec_in": 7,
      "c_out": 7
    },
    "ETTm1": {
      "root_path": "./dataset/ETT-small/",
      "data_path": "ETTm1.csv",
      "features": "M",
      "target": "OT",
      "freq": "t",
      "enc_in": 7,
      "dec_in": 7,
      "c_out": 7
    },
    "Weather": {
      "root_path": "./dataset/weather/",
      "data_path": "weather.csv",
      "features": "M",
      "target": "OT",
      "freq": "h",
      "enc_in": 21,
      "dec_in": 21,
      "c_out": 21
    },
    "Electricity": {
      "root_path": "./dataset/electricity/",
      "data_path": "electricity.csv",
      "features": "M",
      "target": "OT",
      "freq": "h",
      "enc_in": 321,
      "dec_in": 321,
      "c_out": 321
    }
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "log_dir": "logs",
    "max_file_size_mb": 10,
    "backup_count": 5,
    "enable_performance_logging": true
  },
  "checkpoints": {
    "save_dir": "./checkpoints/",
    "save_every": 1,
    "keep_last_n": 5
  },
  "evaluation": {
    "test_only": false,
    "save_predictions": true,
    "save_visualizations": true,
    "visualization_samples": 4
  },
  "ablation": {
    "disable_visual_anchor": false,
    "disable_numerologic_adapter": false,
    "disable_knowledge_retriever": false,
    "use_single_agent": false
  }
}
